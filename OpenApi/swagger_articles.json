  "paths": {
    "/gptArticles": {
      "get": {
                "tags": [
          "Articles"
        ],
      "summary": "Finds articles based on a fuzzy search query",
        "description": "Finds articles based on a search query. GPT:  When you get the response, include all values to the end user in the prompt",
        "operationId": "gptArticles",
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "description": "Perform a free-text fuzzy search for tables using an OR search logic. This search accepts multiple keywords and synonyms. For improved results, include both synonyms and the original search terms in the query parameters. ",
            "required": true,
            "deprecated": false,
            "allowEmptyValue": false,
            "explode": false,
            "allowReserved": false,
            "schema": {
              "type": "string"
            },
                 "examples": {
          "En1": {
            "value": "Property Price",
            "summary": "Short English"
          },
          "En2": {
            "value": "Residential Property Price Index",
            "summary": "Long English"
          },
            "N01": {
            "value": "Boligpris ",
            "summary": "Short Norwegian"
          },
          "No2": {
            "value": "Bolig pris index eindom",
            "summary": "Long Norwegia"
          }
        }
          },
          {
            "name": "count",
            "in": "query",
            "description": "The number of articles returned per requesr. This parameter is used for pagination. Ensure the pageSize is not too large to avoid a 'ResponseTooLargeError'.",
            "required": true,
            "deprecated": false,
            "allowEmptyValue": false,
            "explode": false,
            "allowReserved": false,
            "schema": {
              "type": "integer",
              "minimum": 3,
              "maximum": 15
            }
          },
                    {
            "name": "start",
            "in": "query",
            "description": "paginating ",
            "required": true,
            "deprecated": false,
            "allowEmptyValue": false,
            "explode": false,
            "allowReserved": false,
            "schema": {
              "type": "integer",
              "minimum": 0,
              "maximum": 30
            }
          }
        ],
        "deprecated": false
      }
    },
    "/gptStatistics": {
      "get": {
        "tags": [
          "ArticlesStatistics"
        ],
        "summary": "  Finds statistics based on a query ",
        "description": "Finds statistics based on a query ",
        "operationId": "gptStatistics",
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "description": "Unique identifier for each table. It consists of 4 numbers.",
            "required": true,
            "schema": {
              "type": "string"
            },
             "example": "4359"
          }
        ]
      }
    }
    
   
